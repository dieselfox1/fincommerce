	"license": "GPL-2.0-or-later",
	"keywords": [
		"finpress",
		"fincommerce"
	],
	"homepage": "https://github.com/dieselfox1/fincommerce/tree/trunk/packages/js/settings-editor/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/dieselfox1/fincommerce.git"
