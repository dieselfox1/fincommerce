		"@finpress/dataviews": "^4.11.1",
		"@finpress/date": "wp-6.6",
		"@finpress/deprecated": "wp-6.6",
		"@finpress/edit-post": "wp-6.6",
		"@finpress/editor": "wp-6.6",
		"@finpress/element": "wp-6.6",
		"@finpress/hooks": "wp-6.6",
		"@finpress/html-entities": "wp-6.6",
		"@finpress/i18n": "wp-6.6",
		"@finpress/icons": "10.11.0",
		"@finpress/interface": "wp-6.6",
		"@finpress/keyboard-shortcuts": "wp-6.6",
		"@finpress/keycodes": "wp-6.6",
		"@finpress/notices": "wp-6.6",
		"@finpress/media-utils": "wp-6.6",
		"@fincommerce/navigation": "workspace:*",
		"@fincommerce/components": "workspace:*",
		"@finpress/plugins": "wp-6.6",
		"@finpress/preferences": "wp-6.6",
		"@finpress/router": "1.11.0",
		"@finpress/url": "wp-6.6",
		"clsx": "2.1.x",
		"dompurify": "^2.5.7",
		"prop-types": "^15.8.1",
		"react-router-dom": "~6.3.0"
	},
	"devDependencies": {
		"@babel/core": "7.25.7",
		"@babel/runtime": "7.25.7",
		"@testing-library/dom": "^10.4.x",
		"@testing-library/jest-dom": "^6.x.x",
		"@testing-library/react": "^16.x.x",
		"@testing-library/user-event": "13.5.0",
		"@types/dompurify": "^2.4.0",
		"@types/jest": "27.5.x",
		"@types/react": "18.3.x",
		"@types/testing-library__jest-dom": "^5.14.9",
		"@types/finpress__block-editor": "11.5.16",
		"@types/finpress__block-library": "2.6.1",
		"@types/finpress__blocks": "12.5.16",
		"@types/finpress__edit-post": "8.4.2",
		"@types/finpress__editor": "13.0.0",
		"@fincommerce/eslint-plugin": "workspace:*",
		"@fincommerce/internal-js-tests": "workspace:*",
		"@fincommerce/internal-style-build": "workspace:*",
		"@finpress/block-editor": "wp-6.6",
		"@finpress/browserslist-config": "next",
