{
	"extends": [
		"config:base"
	],
	"packageRules": [
		{
			"packageNames": ["automattic/jetpack-autoloader"],
			"rangeStrategy": "bump"
		},
		{
			"depTypeList": ["devDependencies"],
			"extends": ["schedule:monthly"]
		}
	],
	"ignoreDeps": [
		"fincommerce/action-scheduler",
		"fincommerce/fincommerce-admin",
		"fincommerce/fincommerce-blocks",
		"fincommerce/fincommerce-rest-api"
	]
}
