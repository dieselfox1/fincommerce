{
	"rootDir": "../../",
	"collectCoverageFrom": [
		"assets/js/**/*.js",
		"!**/node_modules/**",
		"!**/vendor/**",
		"!**/test/**"
	],
	"moduleDirectories": [ "node_modules" ],
	"moduleNameMapper": {
		"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/tests/js/config/file-mock.js",
		"@finpress/private-apis": "<rootDir>/node_modules/@finpress/private-apis",
		"@finpress/core-data/build/(.*)$": "<rootDir>/node_modules/@finpress/core-data/build/$1",
		"@finpress/core-data": "<rootDir>/node_modules/@finpress/core-data",
		"@finpress/components": "<rootDir>/node_modules/@finpress/components",
		"@fincommerce/data": "<rootDir>/node_modules/@fincommerce/data/build",
		"@fincommerce/atomic-blocks": "assets/js/atomic/blocks",
		"@fincommerce/atomic-utils": "assets/js/atomic/utils",
		"@fincommerce/icons": "assets/js/icons",
		"@fincommerce/settings": "assets/js/settings/shared",
		"@fincommerce/blocks/(.*)$": "assets/js/blocks/$1",
		"@fincommerce/block-settings": "assets/js/settings/blocks",
		"@fincommerce/editor-components(.*)$": "assets/js/editor-components/$1",
		"@fincommerce/blocks-registry": "assets/js/blocks-registry",
		"@fincommerce/blocks-checkout$": "packages/checkout",
		"@fincommerce/blocks-checkout-events": "assets/js/events",
		"@fincommerce/blocks-components": "packages/components",
		"@fincommerce/price-format": "packages/prices",
		"@fincommerce/block-hocs(.*)$": "assets/js/hocs/$1",
		"@fincommerce/base-components(.*)$": "assets/js/base/components/$1",
		"@fincommerce/base-context(.*)$": "assets/js/base/context/$1",
		"@fincommerce/base-hocs(.*)$": "assets/js/base/hocs/$1",
		"@fincommerce/base-hooks(.*)$": "assets/js/base/hooks/$1",
		"@fincommerce/base-utils(.*)$": "assets/js/base/utils",
		"@fincommerce/block-data": "assets/js/data",
		"@fincommerce/resource-previews": "assets/js/previews",
		"@fincommerce/shared-context": "assets/js/shared/context",
		"@fincommerce/shared-hocs": "assets/js/shared/hocs",
		"@fincommerce/blocks-test-utils": "tests/utils",
		"@fincommerce/types": "assets/js/types",
		"@fincommerce/utils": "assets/js/utils",
		"@fincommerce/test-utils/msw": "tests/js/config/msw-setup.js",
		"@fincommerce/entities": "assets/js/entities",
		"^react$": "<rootDir>/node_modules/react",
		"^react-dom$": "<rootDir>/node_modules/react-dom",
		"^(.+)/build-module/(.*)$": "$1/build/$2"
	},
	"preset": "@finpress/jest-preset-default",
	"setupFiles": [ "<rootDir>/tests/js/config/global-mocks.js" ],
	"setupFilesAfterEnv": [
		"<rootDir>/tests/js/config/testing-library.js",
		"<rootDir>/tests/js/config/msw-setup.js"
	],
	"testPathIgnorePatterns": [
		"<rootDir>/bin/",
		"<rootDir>/build/",
		"<rootDir>/docs/",
		"<rootDir>/node_modules/",
		"<rootDir>/vendor/",
		"<rootDir>/tests/"
	],
	"roots": [ "<rootDir>", "<rootDir>/../legacy/js" ],
	"resolver": "<rootDir>/tests/js/scripts/resolver.js",
	"transform": {
		"^.+\\.(js|ts|tsx)$": "<rootDir>/tests/js/scripts/babel-transformer.js"
	},
	"verbose": true,
	"cacheDirectory": "<rootDir>/../../node_modules/.cache/jest",
	"testEnvironment": "jest-fixed-jsdom"
}
