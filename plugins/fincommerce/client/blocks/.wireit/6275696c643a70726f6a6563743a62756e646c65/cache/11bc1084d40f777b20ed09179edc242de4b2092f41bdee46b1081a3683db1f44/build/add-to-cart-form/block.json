{
	"name": "fincommerce/add-to-cart-form",
	"title": "Add to Cart with Options",
	"description": "Display a button that lets customers add a product to their cart. Use the added options to optimize for different product types.",
	"category": "fincommerce-product-elements",
	"attributes": {
		"quantitySelectorStyle": {
			"type": "string",
			"enum": [ "input", "stepper" ],
			"default": "input"
		}
	},
	"keywords": [ "FinCommerce" ],
	"usesContext": [ "postId" ],
	"textdomain": "fincommerce",
	"supports": {
		"interactivity": true
	},
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"viewScriptModule": "fincommerce/add-to-cart-form",
	"style": "file:../fincommerce/add-to-cart-form-style.css",
	"editorStyle": "file:../fincommerce/add-to-cart-form-editor.css"
}
