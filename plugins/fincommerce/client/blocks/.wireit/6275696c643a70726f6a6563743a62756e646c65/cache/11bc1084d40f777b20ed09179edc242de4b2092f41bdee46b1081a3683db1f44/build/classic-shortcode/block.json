{
	"name": "fincommerce/classic-shortcode",
	"title": "Classic Shortcode",
	"description": "Renders classic FinCommerce shortcodes.",
	"category": "fincommerce",
	"keywords": [ "FinCommerce" ],
	"supports": {
		"interactivity": {
			"clientNavigation": false
		},
		"align": true,
		"html": false,
		"multiple": false,
		"reusable": false,
		"inserter": true
	},
	"attributes": {
		"shortcode": {
			"type": "string",
			"default": "cart",
			"enum": [ "cart", "checkout" ]
		},
		"align": {
			"type": "string",
			"default": "wide"
		}
	},
	"textdomain": "fincommerce",
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
