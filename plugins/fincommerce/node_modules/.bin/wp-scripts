#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/@wordpress+scripts@30.6.0_@playwright+test@1.50.1_@swc+core@1.3.100_@types+node@22.9.1_@types_sfpea65il2xxj2bn4oadq2czke/node_modules/@wordpress/scripts/bin/node_modules:/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/@wordpress+scripts@30.6.0_@playwright+test@1.50.1_@swc+core@1.3.100_@types+node@22.9.1_@types_sfpea65il2xxj2bn4oadq2czke/node_modules/@wordpress/scripts/node_modules:/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/@wordpress+scripts@30.6.0_@playwright+test@1.50.1_@swc+core@1.3.100_@types+node@22.9.1_@types_sfpea65il2xxj2bn4oadq2czke/node_modules/@wordpress/node_modules:/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/@wordpress+scripts@30.6.0_@playwright+test@1.50.1_@swc+core@1.3.100_@types+node@22.9.1_@types_sfpea65il2xxj2bn4oadq2czke/node_modules:/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/@wordpress+scripts@30.6.0_@playwright+test@1.50.1_@swc+core@1.3.100_@types+node@22.9.1_@types_sfpea65il2xxj2bn4oadq2czke/node_modules/@wordpress/scripts/bin/node_modules:/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/@wordpress+scripts@30.6.0_@playwright+test@1.50.1_@swc+core@1.3.100_@types+node@22.9.1_@types_sfpea65il2xxj2bn4oadq2czke/node_modules/@wordpress/scripts/node_modules:/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/@wordpress+scripts@30.6.0_@playwright+test@1.50.1_@swc+core@1.3.100_@types+node@22.9.1_@types_sfpea65il2xxj2bn4oadq2czke/node_modules/@wordpress/node_modules:/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/@wordpress+scripts@30.6.0_@playwright+test@1.50.1_@swc+core@1.3.100_@types+node@22.9.1_@types_sfpea65il2xxj2bn4oadq2czke/node_modules:/home/administrator/Documents/GitHub/woocommerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@wordpress/scripts/bin/wp-scripts.js" "$@"
else
  exec node  "$basedir/../@wordpress/scripts/bin/wp-scripts.js" "$@"
fi
